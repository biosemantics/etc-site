<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my='urn:import:edu.arizona.sirls.etc.site.client.view.fileManager'>
	<ui:style>      
		.fileTree {
			height: 300px;
			max-height: 300px;
			max-width: 900px;
			min-width: 900px;
			width:900px;
		}
	</ui:style>
	<g:ScrollPanel styleName="{style.fileTree}">
		<my:FileImageLabelTree ui:field='fileTree'></my:FileImageLabelTree>
	</g:ScrollPanel> 
</ui:UiBinder> 