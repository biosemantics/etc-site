<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:edu.arizona.biosemantics.etcsite.client.common"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
    xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container">
	
	<ui:style>
		.clickable {
			cursor:pointer;
		}
		.html {
			width: 100%;
		}
	</ui:style>
	<ui:with type="com.sencha.gxt.widget.core.client.ContentPanel.ContentPanelAppearance" field="contentPanelAppearance" />
	<g:VerticalPanel>
	<g:Button ui:field = "openButton">Open in new window</g:Button>
	<gxt:ContentPanel ui:field="panel" appearance="{contentPanelAppearance}" bodyBorder="false" headingText="Help/Instructions" pixelSize="360, 550" >
	
	    
  </gxt:ContentPanel>
  </g:VerticalPanel>
</ui:UiBinder> 
	